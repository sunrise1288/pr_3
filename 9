import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Введите начальный пробег (x): ");
        double x = scanner.nextDouble();

        System.out.print("Введите целевой пробег (y): ");
        double y = scanner.nextDouble();

        int day = 1;
        while (x < y) {
            x = x * 1.1;
            day++;
        }

        System.out.println("На " + day + "-й день пробег спортсмена составит не менее " + y + " километров.");
    }
}
