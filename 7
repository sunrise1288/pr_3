import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Введите целое число (не меньше 2): ");
        int n = scanner.nextInt();

        if (n < 2) {
            System.out.println("Число должно быть не меньше 2.");
        } else {
            int smallestDivisor = findSmallestDivisor(n);
            System.out.println("Наименьший натуральный делитель числа " + n + " (отличный от 1) = " + smallestDivisor);
        }
    }

    public static int findSmallestDivisor(int n) {
        for (int i = 2; i <= Math.sqrt(n); i++) {
            if (n % i == 0) {
                return i;
            }
        }
        return n; // Если число простое, его наименьший делитель - само число
    }
}
