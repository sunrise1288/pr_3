import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        int max = Integer.MIN_VALUE; // Инициализируем максимум минимальным значением int
        
        System.out.println("Введите последовательность различных натуральных чисел (введите 0 для завершения):");
        
        while (true) {
            int num = scanner.nextInt();
            
            if (num == 0) {
                break;
            }
            
            if (num > max) {
                max = num;
            }
        }
        
        if (max == Integer.MIN_VALUE) {
            System.out.println("Вы не ввели ни одного числа.");
        } else {
            System.out.println("Максимальный элемент в последовательности: " + max);
        }
    }
}
